    "description": "My personal light theme.",
    "version": "1.0.0",
    "author": "Jay Soren <admin@fmotiondev.com>",
    "publisher": "Futuremotion",
    "icon": "./img/extensionIcon.png",
    "galleryBanner": {
        "color": "#E3E6E9",
        "theme": "light"
    },
    "license": "MIT",
    "homepage": "https://github.com/fmotion1/Futuremotion-Light",
    "repository": {
        "type": "git",
